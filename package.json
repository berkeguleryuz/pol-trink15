{
  "name": "polybuy",
  "version": "1.0.0",
  "description": "Polymarket Trading Bot - Automated share trading on Polymarket prediction markets",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "bot:dry": "ts-node src_new/bot/main.ts",
    "bot:live": "ts-node src_new/bot/main.ts --live",
    "bot:telegram": "ts-node src_new/bot/main.ts --live telegram",
    "copy:dry": "ts-node src_new/copy-trading/copy-trading-bot.ts",
    "copy:live": "ts-node src_new/copy-trading/copy-trading-bot.ts --live",
    "copy:ws:dry": "ts-node src_new/copy-trading/copy-trading-bot-ws.ts",
    "copy:ws:live": "ts-node src_new/copy-trading/copy-trading-bot-ws.ts --live",
    "copy:fast:dry": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts",
    "copy:fast:dry:1": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts --fixed=1",
    "copy:fast:dry:up": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts --mintrade=0.6",
    "copy:fast:dry:up:1": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts --fixed=1 --mintrade=0.6",
    "copy:fast:live": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts --live",
    "copy:fast:live:1": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts --live --fixed=1",
    "copy:fast:live:up": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts --live --mintrade=0.6",
    "copy:fast:live:up:1": "ts-node src_new/copy-trading/copy-trading-bot-fast.ts --live --fixed=1 --mintrade=0.6",
    "sniper:dry": "ts-node src_new/copy-trading/sniper-bot.ts",
    "sniper:live": "ts-node src_new/copy-trading/sniper-bot.ts --live",
    "sniper:clob:dry": "ts-node src_new/copy-trading/sniper-bot-clob.ts",
    "sniper:clob:live": "ts-node src_new/copy-trading/sniper-bot-clob.ts --live",
    "transfer:usdc": "ts-node src_new/scripts/transfer-usdc.ts",
    "analyze:trades": "ts-node src_new/scripts/analyze-trades.ts",
    "claim:check": "ts-node src_new/scripts/manual-claim.ts",
    "claim:all": "ts-node src_new/scripts/manual-claim.ts --claim",
    "claim:gasless": "ts-node src_new/scripts/claim-gasless.ts",
    "claim:gasless:run": "ts-node src_new/scripts/claim-gasless.ts --claim",
    "test:prices": "ts-node src_new/scripts/test-crypto-prices.ts",
    "test:ws": "ts-node src_new/scripts/test-ws-prices.ts",
    "whale:debug": "ts-node src_new/scripts/whale-strategy-debug.ts"
  },
  "keywords": [
    "polymarket",
    "trading",
    "prediction-markets",
    "blockchain",
    "polygon",
    "usdc"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@polymarket/builder-relayer-client": "^0.0.8",
    "@polymarket/clob-client": "^4.22.8",
    "@types/node-telegram-bot-api": "^0.64.12",
    "axios": "^1.6.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "https-proxy-agent": "^7.0.6",
    "node-telegram-bot-api": "^0.66.0",
    "playwright": "^1.56.1",
    "puppeteer-core": "^24.29.0",
    "socks-proxy-agent": "^8.0.5",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
